    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch typescript file",
            "type": "node",
            "request": "launch",
            "program": "${file}"
        },
        {
            "name": "Launch test function",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/compliance",
            "args": ["-test.v"]
        },
        {
            "name": "Compile example program",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/goscript",
            "cwd": "${workspaceFolder}/example/simple",
            "args": [
                "compile",
                "--package",
                "."
            ]
        }
    ]
}