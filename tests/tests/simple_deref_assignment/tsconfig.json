{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "lib": [
      "es2022",
      "esnext.disposable",
      "dom"
    ],
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "paths": {
      "*": [
        "./*"
      ],
      "@goscript/*": [
        "../../../gs/*",
        "../../../tests/deps/*"
      ],
      "@goscript/github.com/aperturerobotics/goscript/tests/tests/simple_deref_assignment/*": [
        "./*"
      ]
    },
    "sourceMap": true,
    "target": "es2022"
  },
  "extends": "../../../tsconfig.json",
  "include": [
    "index.ts",
    "simple_deref_assignment.gs.ts"
  ]
}
