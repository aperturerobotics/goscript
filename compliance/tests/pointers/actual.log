--- Initial Values ---
s1.Val: 1
s2.Val: 2
p1==p2: true
p1==p3: false

--- Pointer Comparisons ---
pp1==pp2: false
pp1==pp3: false
*pp1==*pp2: true
*pp1==*pp3: false
(**pp1).Val == (**pp2).Val: true
(**pp1).Val == (**pp3).Val: true
ppp1==ppp1: true
*ppp1==pp1: true
**ppp1==p1: true
(***ppp1).Val == s1.Val: false

--- Modifications ---
After *p1 = {Val: 10}:
  s1.Val: 10
  (*p2).Val: undefined
  (**pp1).Val: undefined
  (***ppp1).Val: undefined
  s2.Val: 2
After **pp3 = {Val: 20}:
  s2.Val: 20
  (*p3).Val: undefined
  s1.Val: 10

--- Nil Pointers ---
np == nil: true
npp == nil: true
nppp == nil: true
After npp = &np:
  npp == nil: false
  *npp == nil: true